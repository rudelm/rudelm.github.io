	<!-- Login lightbox -->

	<div id="logInLightbox" class="hidden">
		<div class="loading">Loading…</div>
		<iframe scrolling=no class="iframe" src="" marginheight="0" marginwidth="0" frameborder="0" id="iframeLogin"></iframe>
	</div>


	<script type="text/javascript">
	$j(document).ready( function(){
		//Checks if Ext-JS is loaded	 
		if ( typeof Ext != 'undefined' ){ 
		 
			var checkCookie = Ext.util.Cookies.get("ue");
		 
			//If the cookie is null, the user is not logged and we show the login lightbox
			if( checkCookie == null){
		 
				var logInModal = null;
				 
				if( $j('body').hasClass('responsive-page') ){
					var isSmallDevice, loginWidth, loginHeight; 
					 
					function loginSize(){
						var windowSizeNow = jQuery(window).width();
							//documentHeightNow = jQuery(document).height() + 300;
						 
						isSmallDevice = $j('body').hasClass('small');
					 
						loginWidth = isSmallDevice ? windowSizeNow : 600;
						loginHeight = isSmallDevice ? 'auto' : 390;
						
						//console.log('w: ' + loginWidth);
						//console.log('h: ' + loginHeight);
						
						if(logInModal != null){
							$j('.modal-dialog-viewport').css({
								width: loginWidth,
								height: loginHeight
							});
							var modalIsHidden = $j('#modal-dialog-0').hasClass('hidden');
							if(isSmallDevice){
								$j('.modal-dialog-viewport').removeClass('regularScreenLogin').addClass('smallScreenLogin');
								$j('.modal-dialog-viewport').css('box-shadow','none');  
								if(!modalIsHidden){
									$j('#lightboxHide, .curtain').addClass('hidden');
								}
							}else{
								$j('.modal-dialog-viewport').css('box-shadow','box-shadow: 0 10px 10px #555555');
								$j('.modal-dialog-viewport').removeClass('smallScreenLogin').addClass('regularScreenLogin');
								if(!modalIsHidden){
									$j('#lightboxHide, .curtain').removeClass('hidden');
								}	
							}
						}
					}
					 
					 //defining size on load
					 loginSize();
					 
					 var loginTimer;
					 
					 $j(window).resize(function () {
						clearTimeout(loginTimer);
						loginTimer = setTimeout(loginSize, 100);
					 });
				}else{
	   				loginWidth = 600;
					loginHeight = 390;				
				};
				
				<!-- instantiate an instance of the modal when the page load loading the page -->		
				logInModal = new Ext.EMC.Modal("#logInLightbox", {
					autoCenter:{
						vert:true,
						horz:true
					},
					
	   				width: loginWidth,
					height: loginHeight,
	
					buttons: [{text: 'Close', cls: 'close-btn-small'}],
					closeOnOverlayClick: true,
					destroy: false,
					rootNode : document.getElementsByTagName('body')[0]
				});
				
				<!-- made the modal hidden -->
					if(location.search.indexOf('showLogin=1') >= 0){
                        if( $j('iframe.iframe').attr('src') == '' ){
                            $j('iframe.iframe').attr('src', '/login.htm?jx=true');
                        }
                        logInModal.modalShow();
					} else {
                    	logInModal.modalHide();
                    }
				
				 
				<!-- onClicks events definition -->
				Ext.get(logInModal.getId()).select('.close-btn-small a').on('click', function(ev) { 
					 ev.stopEvent();
					 $j('#lightboxHide, .curtain').removeClass('hidden');	
					 logInModal.modalHide();
					 regularLoginLightboxLayout();
				});
				 
				Ext.get(logInModal.getId()).select('.curtain').on('click', function(ev) { 
					 ev.stopEvent();
					 $j('#lightboxHide, .curtain').removeClass('hidden');	
					 logInModal.modalHide();
					 regularLoginLightboxLayout();
				});
				
				$j('#header, .curtain, .modal-dialog-overlay').live('click', function(){
					$j('#lightboxHide, .curtain').removeClass('hidden');
				});
				 		 
				if( $j('a[href$="/login.htm"]').length > 0 ){
					var button = $j('a[href$="/login.htm"]');
					if( $j('body').hasClass('responsive-page') ){
						button.addClass("openlightbox-responsive");
					}else{
						button.addClass("openlightbox");
					}
				}
				 
					if( !$j('body').hasClass('responsive-page') ){				
						Ext.select('.openlightbox').on('click', function(e, el){
							e.stopEvent();
							if( $j('iframe.iframe').attr('src') == '' ){
								$j('iframe.iframe').attr('src', '/login.htm?jx=true');
							}
							logInModal.modalShow();
						});
					}else{
						Ext.select('.openlightbox-responsive').on('click', function(e, el){
							e.stopEvent();
							 
							if( $j('iframe.iframe').attr('src') == '' ){
								$j('iframe.iframe').attr('src', '/login.htm?jx=true');
							}
	
							logInModal.modalShow();
							
							if(isSmallDevice){
									$j('#headerTop .btn:first').trigger('click');
									$j('#lightboxHide, .curtain').addClass('hidden');
									$j('.modal-dialog-viewport').css('top','44px');
									$j('.modal-dialog-viewport').removeClass('regularScreenLogin').addClass('smallScreenLogin');
						
							}else{
								$j('.modal-dialog-viewport').removeClass('smallScreenLogin').addClass('regularScreenLogin');
							};
	
						});
						
							
					};
					
					$j('#iframeLogin').load(function(){
							$j('.loading').fadeOut('slow').remove();
					});	 
					
					function regularLoginLightboxLayout(){
						$j('#iframeLogin').contents().find('.login-wrapper #errorMesageContainer div').addClass('hidden');
						$j('#iframeLogin').contents().find('.login-wrapper .titleEmployees, .login-wrapper .subtitEmployees, .login-wrapper .usernameEmployees, .login-wrapper .passwordEmployees, .login-wrapper .textUnderPasscodeEmployees').addClass('hidden');
						$j('#iframeLogin').contents().find('.login-wrapper .tittleLogin, .login-wrapper .subtitleLogin, .login-wrapper .usernamelogin, .login-wrapper .passwordLogin').removeClass('hidden');
						$j('#iframeLogin').contents().find('.login-wrapper .form-first-column').css('border-right','1px solid #D9D9D9');
						$j('#iframeLogin').contents().find('.login-wrapper .form-second-column').css('display','block');
						$j('#iframeLogin').contents().find('#errorMessagePwNotRecognized, #errorMessageNTIDPasscodeNotRecognized, #errorMessageToken, #errorMessageUnknown').addClass('hidden');		
					};
				};
			};
	});
	
		
	</script>	


